{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "export": {
      "dependsOn": ["build"],
      "outputs": ["out/**"]
    },
    "deploy": {
      "dependsOn": ["export"]
    }
  }
}
