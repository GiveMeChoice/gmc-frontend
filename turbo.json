{
  "globalEnv": ["$NODE_ENV"],
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "start": {
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "export": {
      "dependsOn": ["build"],
      "outputs": ["out/**"]
    },
    "deploy": {
      "dependsOn": ["export"]
    },
    "blog#build": {
      "dependsOn": ["^build"],
      "env": ["NEXT_PUBLIC_SANITY_DATASET", "NEXT_PUBLIC_SANITY_PROJECT_ID"]
    }
  }
}
